	e:=0;
	loop
		if (not [a<c and b<=d]) then
			print a;
			print b;
			print c;
			print d;
			exit 
		endif;
		d := d + 1;
		if (a=e) then
			c:=c+e
		else
			loop
				if (not [a<=d] or a=2) then
					print a;
					exit
				endif;
				a:=a+b
			endloop
		endif;
		c := c + 1;
	endloop;
	temp:=px;
	x:=1;
	y:=2;
	print x;
	print y
endprogram