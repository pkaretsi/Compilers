	function hasnested(in theta, inout r, inandout p)
		
		function alpha(in par1)
			if(par1>2) then
				print par1*2;
				par1 := par1 - 1
			else
				par1 := par1 + 1;
				print par1
			endif;
			return par1
		endfunction
		
		a := 1;
		while (a<5)
			a := a + 1;
			b := alpha(in a)
		endwhile;
		return a
	endfunction

	w:=4;
	dowhile
		if (w>=6) then
			z := w+2
		endif;
		w := w+1
	enddowhile (w>12);
	
	loop
