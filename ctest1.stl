program ctest1
    declare c,a,b,t ;// enddeclare
    a:=1;
	c:=4;
	b:=5;	
	b := b + 1;
	//added while loop with nested forcase statement
	while (b>5 and b<10)
		b := b + 1;
		forcase
			when (b > 5): print b-1
			when (b < 5): print b+2
			default: print a enddefault
		endforcase
		print c;
		print a;
			
		
		
		while (a<3)
			if (a=2) then
				print a;
				print c;
				while(b=1)
					c:=3*a + 3*b;
					print c;
					//added extra while loop
					while(c <> 2)
						c := c - 1
						print c
					endwhile
					//added nested loops
					loop
						t:=c
						if(t>a) then
							loop
								if(t>1) then
									t := 3;
									exit
								endif;
								a :=123
							endloop;
							print t;
							exit
						endif;
						t := t*3
					endloop;
				endwhile
			endif
			//added incase statement
			incase
				when (a+3 <> 4): print a				
			endincase;	
			
			while (a+b<=3 and b<5)
				if (a=1) then
					c:=2;
						
					while (b<10)
						b := b + 3
						print b;
						print c;
					endwhile
				else 
					if (a=2) then
						c:=4;
						print c;			
					else 
						c:=0;
						print c
					endif
				endif;
			endwhile;
			a:= a+10
		endwhile;		
				
	endwhile


	print a;
	print b
endprogram